<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
  
<hibernate-mapping>
    <class name="com.gsm.domain.sub.Client" table="client">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="native" />
        </id>
		
        <property column="guid" name="guid" type="java.lang.String" />
        <property column="servicenum" name="serviceNumber" type="java.lang.String" />
        <property column="firstname" name="firstName" type="java.lang.String" />
        <property column="lastname" name="lastName" type="java.lang.String" />
        <property column ="address" name = "address" type = "java.lang.String"/>
        <property column ="area" name = "area" type = "java.lang.String"/>
        <property column ="wardnum" name = "wardNum" type = "java.lang.String"/>
        <property column ="mobile" name = "mobile" type = "java.lang.String"/>
        <property column ="email" name = "email" type = "java.lang.String"/> 
        <property column ="meternum" name = "meterNumber" type = "java.lang.String"/>
        <property column="createdby" name="createdBy" type="java.lang.Integer" />
        <property column="createdon" name="createdOn" type="java.util.Date" />
        <property column="modifiedby" name="modifiedBy" type="java.lang.Integer" />
        <property column="modifiedon" name="modifiedOn" type="java.util.Date" />
        <property column="active" name="active" type="java.lang.Integer" />
		
		
    </class>
</hibernate-mapping>