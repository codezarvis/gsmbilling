<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
  
<hibernate-mapping>
    <class name="com.gsm.domain.sub.AppUser" table="appuser">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="native" />
        </id>
		
        <property column="guid" name="guid" type="java.lang.String" />
        <property column="username" name="userName" type="java.lang.String" />
        <property column="password" name="password" type="java.lang.String" />
        <property column ="question" name = "question" type = "java.lang.String"/>
        <property column ="answer" name = "answer" type = "java.lang.String"/> 
        <property column ="number" name = "number" type = "java.lang.String"/>
        <property column ="email" name = "email" type = "java.lang.String"/>
        <property column="userrole" name="userRole" type="java.lang.String" />
        <property column="createdby" name="createdBy" type="java.lang.Integer" />
        <property column="createdon" name="createdOn" type="java.util.Date" />
        <property column="modifiedby" name="modifiedBy" type="java.lang.Integer" />
        <property column="modifiedon" name="modifiedOn" type="java.util.Date" />
        <property column="active" name="active" type="java.lang.Integer" />
		
		
    </class>
</hibernate-mapping>