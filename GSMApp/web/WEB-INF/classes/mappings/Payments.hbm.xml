<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
  
  
<hibernate-mapping>
    <class name="com.gsm.domain.sub.Payments" table="payments">
        <id column="id" name="id" type="java.lang.Integer">
            <generator class="native" />
        </id>
		
        <property column="guid" name="guid" type="java.lang.String" />
        <property column="meterId" name="meterId" type="java.lang.String" />
        <property column="clientName" name="clientName" type="java.lang.String" />
        <property column="bank" name="bankName" type="java.lang.String" />
        <property column ="cardNum" name = "cardNumber" type = "java.lang.String"/>
        <property column ="billDate" name = "billDate" type = "java.util.Date"/>
        <property column ="billAmount" name = "billAmount" type = "java.lang.Double"/>
        <property column ="amntPaid" name = "amountPaid" type = "java.lang.Double"/> 
        <property column ="balance" name = "balance" type = "java.lang.Double"/>
        <property column="createdon" name="createdOn" type="java.util.Date" />
        <property column="createdby" name="createdBy" type="java.lang.Integer" />
        <property column="modifiedon" name="modifiedOn" type="java.util.Date" />
        <property column="modifiedby" name="modifiedBy" type="java.lang.Integer" />
        <property column="active" name="active" type="java.lang.Integer" />
		
		
    </class>
</hibernate-mapping>